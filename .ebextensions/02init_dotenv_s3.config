container_commands:
    01init_dotenv_aws_access_key_id:
        command: "echo \"AWS_ACCESS_KEY_ID=$AWS_ACCESS_KEY_ID\" >> .env"
    02init_dotenv_aws_secret_access_key:
        command: "echo \"AWS_SECRET_ACCESS_KEY=$AWS_SECRET_ACCESS_KEY\" >> .env"
    03init_dotenv_aws_default_region:
        command: "echo \"AWS_DEFAULT_REGION=$AWS_DEFAULT_REGION\" >> .env"
    04init_dotenv_aws_bucket:
        command: "echo \"AWS_BUCKET=$AWS_BUCKET\" >> .env"